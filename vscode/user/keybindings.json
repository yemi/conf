[
  {
    "key": "ctrl+cmd+j",
    "command": "extension.vscode-git-automator.addAndCommitAllFiles"
  },
  {
    "key": "shift+cmd+a",
    "command": "-extension.vscode-git-automator.addAndCommitAllFiles"
  },
  {
    "key": "ctrl+cmd+k",
    "command": "extension.vscode-git-automator.addAndCommitCurrentFile"
  },
  {
    "key": "shift+cmd+z",
    "command": "-extension.vscode-git-automator.addAndCommitCurrentFile"
  },
  {
    "key": "ctrl+cmd+l",
    "command": "extension.vscode-git-automator.pushLocalCommits"
  },
  {
    "key": "shift+cmd+x",
    "command": "-extension.vscode-git-automator.pushLocalCommits"
  },
  {
    "key": "ctrl+cmd+h",
    "command": "git.checkout"
  },
  {
    "key": "ctrl+cmd+s",
    "command": "git.branch"
  },
  {
    "key": "[IntlBackslash]",
    "command": "extension.vim_escape",
    "when": "editorTextFocus && vim.active && !inDebugRepl"
  },
  {
    "key": "escape",
    "command": "-extension.vim_escape",
    "when": "editorTextFocus && vim.active && !inDebugRepl"
  },
  {
    "key": "space w",
    "command": "extension.vim_ctrl+w",
    "when": "editorTextFocus && vim.active && vim.mode != 'Insert' && vim.use<C-w> && !inDebugRepl"
  },
  {
    "key": "ctrl+w",
    "command": "-extension.vim_ctrl+w",
    "when": "editorTextFocus && vim.active && vim.use<C-w> && !inDebugRepl"
  },
  {
    "key": "space b",
