  },
  {
    "key": "tab space",
    "command": "extension.debugJest",
    "when": "editorTextFocus && vim.active && vim.mode != 'Insert'"
  },
  {
    "key": "tab r",
    "command": "workbench.action.debug.restart",
    "when": "inDebugMode && !focusedSessionIsAttach && editorTextFocus && vim.active && vim.mode != 'Insert' "
  },
  {
    "key": "ctrl+shift+f5",
